{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}\\src",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "Path": "${env:Path};${workspaceFolder}\\src"
      },
    }
  ]
}
